pipeline {
    agent any

    stages {
        stage('git clone') {
            steps {
           git branch: 'main', url: 'https://github.com/Seegi98/terraform-07-30am.git'
            }
        }
        stage('init') {
            steps {
                dir("day-3-custom-network"){
              sh 'terraform init'
                }
            }
        }
                stage('apply') {
            steps {
                dir("day-3-custom-network"){
              sh 'terraform ${couple} -auto-approve'
                }
            }
        }
         //hello
       
    }
}
